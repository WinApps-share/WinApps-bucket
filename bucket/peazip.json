        "res\\bookmarks.txt",
        "res\\conf.txt",
        "res\\conf-lastgood.txt",
        "res\\custedit.txt"
    ],
    "checkver": {
        "github": "https://github.com/peazip/PeaZip"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/peazip/PeaZip/releases/download/$version/peazip_portable-$version.WIN64.zip",
                "extract_dir": "peazip_portable-$version.WIN64"
